{
  "name": "n8n-nodes-reranker-openai",
  "version": "0.2.1",
  "description": "n8n community node for OpenAI-compatible reranking services",
  "keywords": [
    "n8n-community-node-package",
    "openai",
    "reranker",
    "ai",
    "machine-learning",
    "rag",
    "retrieval",
    "langchain"
  ],
  "license": "MIT",
  "homepage": "https://github.com/robinspt/n8n-nodes-reranker-openai",
  "author": {
    "name": "robinspt",
    "email": "46096863+robinspt@users.noreply.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robinspt/n8n-nodes-reranker-openai.git"
  },
  "engines": {
    "node": ">=18.10"
  },
  "main": "index.js",
  "scripts": {
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/OpenAIApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/RerankerOpenAI/RerankerOpenAI.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.1",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
